  },
  "homepage": "https://github.com/SkillSwap-Service/TPL-Discord-Bot#readme",
  "devDependencies": {
    "@types/node": "^18.14.2",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.14.2",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "ascii-table": "^0.0.9",
