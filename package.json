{
  "name": "bot-template",
  "version": "0.1",
  "description": "This Discord Bot Typescript template will be used in every starting Bot project of the Organization.",
  "main": "dist/App.js",
  "scripts": {
    "serve": "nodemon --watch src -e js,ts,json --exec \"ts-node src/App.ts\"",
    "start": "npm install ; node .",
    "build": "tsup src/App.ts --format cjs --clean"
  },
